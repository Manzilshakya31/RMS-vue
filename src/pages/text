              /* @include flex(0 0 25%); */
              max-width: 25%;
              width: 25%;
          }
          /* @media #{ breakpoint-sm} {
              @include flex(0 0 20%);
              max-width: 20%;
              width: 20%;
          } */
          img {
              width: 100%;
              @include trans-duration(500ms);
          }
          .insta-icon {
              position: absolute;
              width: 100%;
              height: 100%;
              background-color: rgba(64, 186, 55, 0.9);
              top: 0;
              left: 0;
              z-index: 10;
              cursor: pointer;
              opacity: 0;
              visibility: hidden;
